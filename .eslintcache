ucts, setProducts] = useState([]);\n    const { id } = useParams();\n\n    useEffect(() => {\n        let mounted = true;\n        getProductsByCategory(id)\n          .then(item => {\n            if(mounted) {\n              setProducts(item);\n            }\n          })\n        return () => mounted = false;\n      },[])  \n    \n    return (\n        <Container>\n            <Header/>\n            <Wrapper>\n                { products !== null &&\n                    products.map( item => {\n                        return (\n                            <div key={item.id}>\n                                <ProductCard  id={item.id} name={item.name} price={item.price}/>\n                            </div>\n                        );\n                    })\n                }\n            </Wrapper>\n            <Navigation/>\n        </Container>\n    )\n}\n\nexport default CategoryItems;","/home/leonardovictorribeiro/github/cardapio-virtual-frontend/src/pages/Customer/ProductDetail/index.js",["146"],"import React, { useState, useEffect} from 'react';\nimport { Container, Wrapper, Table } from './styles';\nimport Header from '../../../components/Header';\nimport Navigation from '../../../components/Navigation';\nimport { useHistory, useParams } from 'react-router-dom';\nimport { getProduct } from '../../../services/api';\n\nconst ProductDetail = (props) => {\n    const history = useHistory();\n    const [product, setProduct] = useState([]);\n    const [total, setTotal] = useState(100);\n    const [unit, setUnit] = useState(1);\n    const { id } = useParams();\n\n    useEffect(() => {\n        let mounted = true;\n        getProduct(id)\n          .then(item => {\n            if(mounted) {\n              setProduct(item);\n              setTotal(item.price);\n            }\n          })\n\n        return () => mounted = false;\n      }, [])\n    \n      function update(e){\n        setTotal(e.target.value * product.price);\n        setUnit(e.target.value);\n        e.preventDefault();\n      }\n\n    \n\n    return(\n        <Container>\n            <Header/>\n            <Wrapper>\n                <h1>{ product.name }</h1>\n                <p>{ product.description }</p>\n                <Table>\n                    <tr>\n                        <th>Unidade</th>\n                        <th>Valor</th>\n                    </tr>\n                    <tr>\n                        <td>\n                            <select onChange={update}>\n                                <option value=\"1\">1</option>\n                                <option value=\"2\">2</option>\n                                <option value=\"3\">3</option>\n                            </select>\n                        </td>\n                        <td>{`R$ ${total}`}</td>\n                    </tr>\n                </Table>\n                <button onClick={() => {\n                    var order = JSON.parse(localStorage.getItem('order'));\n                    \n                    if (order !== null){\n                        order.forEach( item => {\n                            if (item.id === product.id){\n                                order.pop(item);\n                            }\n                        });\n                    }\n                    order.push({id:product.id, name: product.name, quantity: unit, value: total});\n                    localStorage.setItem('order', JSON.stringify(order))\n                    history.push('/order');\n                }} >Adicionar ao pedido</button>\n            </Wrapper>\n            <Navigation/>\n        </Container>\n    );\n}\n\nexport default ProductDetail;","/home/leonardovictorribeiro/github/cardapio-virtual-frontend/src/pages/Customer/Main/index.js",[],"/home/leonardovictorribeiro/github/cardapio-virtual-frontend/src/pages/Customer/Login/index.js",[],"/home/leonardovictorribeiro/github/cardapio-virtual-frontend/src/pages/Customer/Order/index.js",["147","148"],"/home/leonardovictorribeiro/git